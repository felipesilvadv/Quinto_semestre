dsev = function(x, location = 0, scale = 1){
mu = location
sigma = scale
z = (x - mu)/sigma
aux = exp(z-exp(z))/sigma
aux
}
psev = function(q, location = 0, scale = 1){
mu = location
sigma = scale
z = (q - mu)/sigma
aux = 1-exp(-exp(z))
aux
}
qsev = function(p, location = 0, scale = 1){
mu = location
sigma = scale
aux = mu + sigma * log(-log(1-p))
aux
}
